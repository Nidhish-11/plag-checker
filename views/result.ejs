<style>
    
     
 
  .b-skills
  {
    /* border-top: 1px solid #f9f9f9; */
    padding-top: 46px;
    text-align: center;
  }
  
  .b-skills:last-child { margin-bottom: -30px; }
  
  .b-skills h2 { margin-bottom: 50px; font-weight: 900; text-transform: uppercase;}
  
  .skill-item
  {
    position: relative;
    max-width: 250px;
    width: 100%;
    margin-bottom: 30px;
    color: white;
  }
  
  .chart-container
  {
    position: relative;
    width: 100%;
    height: 0;
    padding-top: 100%;
    margin-bottom: 27px;
  }
  
  .skill-item .chart,
  .skill-item .chart canvas
  {
    position: absolute;
    top: 0;
    left: 0;
    width: 100% !important;
    height: 100% !important;
  }
  
  .skill-item .chart:before
  {
    content: "";
    width: 0;
    height: 100%;
  }
  
  .skill-item .chart:before,
  .skill-item .percent
  {
    display: inline-block;
    vertical-align: middle;
  }
  
  .skill-item .percent
  {
    position: relative;
    line-height: 1;
    font-size: 40px;
    font-weight: 900;
    z-index: 2;
  }
  
  .skill-item  .percent:after
  {
    content: attr(data-after);
    font-size: 20px;
  }
         
         p{
             font-weight: 900;
         }
   
      
      </style>

<body><div class="background">
  
  <%- include('./partials/header.ejs') %>

  <main>
    <section class="hero">
     
  
      <div class="resultscon1">
        <div class="inputcon">          
          <p style="font-size: 20px;"><label class=name><strong>Name:</strong></label> <%= name %></p>     
          
          <h2><strong >Plagiarism Score:</strong> <%= score %></h2>
        </div>
        </div>
    
      <div class="res-div">
           Total Words&emsp;    
    
           Identical Words&emsp;

           Related Meaning Words&emsp;
        
           Minor Changed Words&emsp;
        </div>
        <div class="no"><%= totalWords %>&emsp;&emsp;&emsp;&emsp;<%= identicalWords %>&emsp;&emsp;&emsp;&emsp;&emsp;&nbsp;&nbsp;<%= relatedMeaningWords %>&emsp;&emsp;&emsp;&emsp;&emsp;&emsp;&nbsp; <%= minorChangedWords %></div>
        <div>
          
        
            <span id="so" class="source">SOURCE</span>&nbsp;&nbsp;&nbsp;
              <span id="ti" class="title">TITLE</span>&nbsp;&nbsp;&nbsp;
              <span id="ma" class="matched">WORDS</span>
        
          
        </div>
    
        <ul class="source-list">
          <% if(internet.length > 0) { %>
            <% internet.forEach(source => { %>
             
              <li>
                <span class="sources"><%= source.url %></span>
                <span class="titles"><%= source.title %></span>
                <span class="match-words"><%= source.matchedWords %></span>
              </li>

            <% }) %>
          <% } %>
        </ul>
        

      <!-- <div class =" plagimg1">
        <img src="./3297225.png" width="200" height="200">
    </div> -->
      
    </section>
  </main>
  

  <footer>
    <p>&copy; 2023 AYUSHMAN SYSTEM FOR MEDICINE</p>
  </footer>
</div>
<div class ="plagimg1">
<div class="b-skills">
  <div class="container">
    

    <div class="row">
      <div class="col-xs-12 col-sm-6 col-md-3">
        <div class="skill-item center-block">
          <div class="chart-container">
            <div class="chart " data-percent="<%= score %>" data-bar-color="#cc0000">
              <span class="percent" data-after="%"><%= score %></span>
            </div>
          </div>

        </div>
      </div>



      </div>
    </div>
  </div>
  </div>>
</div>
